mixin bulletList(options, items)
  div(class="bulletList__wrapper")
    h1(class=`bulletList__title bulletList__title_${options.titleMod} bulletList__title_${options.additionalTitleMod} bulletList__title_${options.positionMod}`)=options.title
    ul(class="bulletList")
      each item in items
        li(class="bulletList__item")!=item